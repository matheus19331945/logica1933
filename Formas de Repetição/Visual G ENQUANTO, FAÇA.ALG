Algoritmo "semnome"

Var
num:inteiro


Inicio
       num <- 1  //VARIÁVEL DE CONTROLE!!!! PARA CONTROLAR O ALGORITHM
       
       enquanto (num <= 1000) faca    //
                                      // <INSTRUÇÕES ENQUANTO SE MANTÊM O LOOP
             escreval (num)           //
       num <- num+1                   //
       
       fimenquanto


Fimalgoritmo